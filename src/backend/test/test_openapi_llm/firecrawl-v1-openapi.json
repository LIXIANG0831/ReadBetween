{
  "openapi": "3.0.0",
  "info": {
    "title": "EMA-空压报告图表工具API",
    "description": "提供空压机相关数据的查询接口",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://energy-report-generation.t.cosmoplat.cn",
      "description": "测试环境"
    }
  ],
  "paths": {
    "/tool/v1/air-compressor/charts": {
      "get": {
        "summary": "获取空压机图表数据",
        "description": "根据站点代码和日期范围查询空压机图表数据",
        "operationId": "getAirCompressorCharts",
        "parameters": [
          {
            "name": "station_code",
            "in": "query",
            "description": "站点代码",
            "required": true,
            "schema": {
              "type": "string",
              "example": "794"
            }
          },
          {
            "name": "start_date",
            "in": "query",
            "description": "开始日期 (YYYY-MM-DD格式)",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date",
              "example": "2025-11-25"
            }
          },
          {
            "name": "end_date",
            "in": "query",
            "description": "结束日期 (YYYY-MM-DD格式)",
            "required": true,
            "schema": {
              "type": "string",
              "format": "date",
              "example": "2025-11-25"
            }
          }
        ],
        "security": []
      }
    }
  },
  "components": {
    "schemas": {},
    "parameters": {},
    "securitySchemes": {}
  }
}