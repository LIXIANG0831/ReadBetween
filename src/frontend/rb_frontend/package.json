{
  "name": "readbetween",
  "version": "0.0.0",
  "packageManager": "pnpm@9.5.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build:strict": "vue-tsc -b && vite build",
    "build": "vite build",
    "preview": "vite preview",
    "openapi": "tsx './src/openapi.ts'",
    "typecheck": "tsc --noEmit && vue-tsc --noEmit --skipLibCheck",
    "clean:cache": "rimraf .eslintcache && rimraf pnpm-lock.yaml && rimraf node_modules && pnpm store prune && pnpm install",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock,build}/**/*.{vue,js,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,tsx,json,css,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{html,vue,css,scss}\" --cache-location node_modules/.cache/stylelint/",
    "lint": "pnpm lint:eslint && pnpm lint:prettier && pnpm lint:stylelint"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@deepgram/sdk": "^3.11.2",
    "@element-plus/icons-vue": "^2.3.1",
    "@kousum/semi-icons-vue": "^0.24.0",
    "@kousum/semi-ui-vue": "^0.25.0",
    "@tdesign-vue-next/chat": "^0.3.0",
    "@vueuse/core": "^10.11.0",
    "ant-design-vue": "^4.2.6",
    "ant-design-x-vue": "^0.0.16",
    "axios": "^1.7.2",
    "echarts": "^5.6.0",
    "element-plus": "^2.7.7",
    "escape-html": "^1.0.3",
    "lodash-es": "^4.17.21",
    "monaco-editor": "^0.52.2",
    "neo4j-driver": "^5.28.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.1.7",
    "prism-theme-vars": "^0.2.5",
    "tdesign-icons-vue-next": "^0.3.6",
    "tdesign-vue-next": "^1.13.1",
    "vue": "^3.4.31",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@iconify/json": "^2.2.230",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it-link-attributes": "^3.0.5",
    "@types/node": "^20.14.12",
    "@types/nprogress": "^0.2.3",
    "@typescript-eslint/eslint-plugin": "^7.17.0",
    "@typescript-eslint/parser": "^7.17.0",
    "@umijs/openapi": "^1.12.1",
    "@vitejs/plugin-vue": "^5.0.5",
    "eslint": "^8.56.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "^9.27.0",
    "markdown-it-link-attributes": "^4.0.1",
    "markdown-it-prism": "^2.3.0",
    "pnpm": "^9.5.0",
    "postcss-px-to-viewport-8-plugin": "^1.2.5",
    "prettier": "^3.3.3",
    "sass": "^1.77.8",
    "stylelint": "^16.7.0",
    "stylelint-config-recess-order": "^5.0.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "tsx": "^4.16.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.18.1",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.27.3",
    "unplugin-vue-markdown": "^0.26.2",
